{
    "name" : "What Not To Wear",
    "version" : "1.0",
    "manifest_version" : 2,
    "icons":{
        "16": "extension-icon.png",
        "32": "extension-icon.png",
        "48": "extension-icon.png",
        "128": "extension-icon.png"
    },
    "content_scripts": [
        {
            "matches" : ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
   
    "browser_action": {               
        "default_popup": "popup.html",                                                                                                                
        "default_title": "What Not To Wear                                                                                   "
    }
}
    
